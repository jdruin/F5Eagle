<Messages Namespace="Eagle._Messages" Resources="Messages" xmlns="http://schemas.microsoft.com/genmsgs/2004/07/messages">
  <Class Name="ScriptErrors" ContainerName="ScriptError" BaseContainerName="Eagle._Components.Public.MessageEventArgs">
    <Message Id="GenericError" Number="1001">
      <Instance>ERROR: '{0}'.
        <Parameter Type="System.String" Name="message" />
      </Instance>
    </Message>
  </Class>

  <Class Name="ScriptWarnings" ContainerName="ScriptWarning" BaseContainerName="Eagle._Components.Public.MessageEventArgs">
    <Message Id="GenericWarning" Number="2001">
      <Instance>WARNING: '{0}'.
        <Parameter Type="System.String" Name="message" />
      </Instance>
    </Message>
  </Class>

  <Class Name="ScriptInformationals" ContainerName="ScriptInformational" BaseContainerName="Eagle._Components.Public.MessageEventArgs">
    <Message Id="GenericInformation" Number="3001">
      <Instance>INFORMATION: '{0}'.
        <Parameter Type="System.String" Name="message" />
      </Instance>
    </Message>
  </Class>
</Messages>
